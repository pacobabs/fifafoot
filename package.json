{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "dev:watch": "cross-env WATCH=true gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --coverage --verbose --colors",
    "lint": "eslint *.{js,ts,tsx} --ignore-path .gitignore .",
    "lint:fix": "eslint *.{js,ts,tsx} --quiet --fix --ignore-path .gitignore ."
  },
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "gatsby": "^2.22.4",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-manifest": "^2.4.8",
    "gatsby-plugin-offline": "^3.2.6",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sharp": "^2.6.8",
    "gatsby-source-filesystem": "^2.3.7",
    "gatsby-transformer-sharp": "^2.5.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "swr": "^0.2.1",
    "use-context-selection": "^1.4.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.4",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-last": "^0.0.5",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-codegen": "^1.1.0",
    "gatsby-plugin-ts-config": "^0.2.2",
    "gatsby-transformer-json": "^2.4.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "remotedev": "^0.2.9",
    "remotedev-utils": "^0.1.6",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "keywords": [
    "gatsby"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-path .gitignore .",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
