{
  "name": "fifafoot",
  "private": true,
  "description": "Fifa match results",
  "version": "0.1.0",
  "author": "Pacobabs <pacobabs@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "dev:watch": "cross-env WATCH=true gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint *.{js,ts,tsx} --ignore-path .gitignore .",
    "lint:fix": "eslint *.{js,ts,tsx} --quiet --fix --ignore-path .gitignore .",
    "test": "jest --coverage --verbose --colors",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "cross-fetch": "^3.0.6",
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-offline": "^3.3.3",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-source-filesystem": "^2.4.2",
    "gatsby-transformer-sharp": "^2.5.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "swr": "^0.3.8",
    "use-context-selection": "^1.4.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.5.16",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-config-last": "^0.0.5",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-codegen": "^1.2.14",
    "gatsby-plugin-ts-config": "^1.1.1",
    "gatsby-transformer-json": "^2.4.15",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-transform-css": "^2.1.0",
    "prettier": "^2.1.2",
    "reduce-devtools-extension": "^2.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-path .gitignore .",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
