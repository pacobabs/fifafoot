{
  "name": "fifafoot",
  "private": true,
  "description": "Fifa match results",
  "version": "0.1.0",
  "author": "Pacobabs <pacobabs@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop -p 3000",
    "dev:watch": "cross-env WATCH=true gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint *.{js,jsx,ts,tsx} --ignore-path .gitignore .",
    "lint:fix": "eslint *.{js,jsx,ts,tsx} --quiet --fix --ignore-path .gitignore .",
    "test": "jest --coverage --verbose --colors",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "type-check": "tsc --pretty --noEmit",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "cross-fetch": "^3.0.6",
    "gatsby": "^2.27.0",
    "gatsby-image": "^2.6.0",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-offline": "^3.5.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-sharp": "^2.9.0",
    "gatsby-source-filesystem": "^2.6.0",
    "gatsby-transformer-sharp": "^2.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-reduce-hooks": "^1.0.4",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-config-last": "^0.0.5",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-codegen": "^1.2.14",
    "gatsby-plugin-postcss": "^3.2.0",
    "gatsby-plugin-ts-config": "^1.1.1",
    "gatsby-transformer-json": "^2.6.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-transform-css": "^2.1.0",
    "lint-staged": "^10.5.1",
    "postcss": "^8.1.8",
    "prettier": "^2.1.2",
    "reduce-devtools-extension": "^2.1.0",
    "tailwindcss": "^2.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix --ignore-path .gitignore .",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests",
      "git add"
    ],
    "*.+(json|yml|yaml|md|mdx|html|css|graphql)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env PRE_COMMIT=true lint-staged",
      "pre-push": "yarn test-all",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
